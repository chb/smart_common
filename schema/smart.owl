            <owl:Restriction>
                <owl:onProperty rdf:resource="&sp;temperature"/>
                <owl:allValuesFrom rdf:resource="&sp;VitalSign"/>
            </owl:Restriction>
        </owl:annotatedTarget>
    </owl:Axiom>
    <owl:Axiom>
        <rdfs:comment>Date + time when vital signs were recorded, as an ISO-8601 string.</rdfs:comment>
        <owl:annotatedSource rdf:resource="&sp;VitalSignSet"/>
        <owl:annotatedProperty rdf:resource="&rdfs;subClassOf"/>
