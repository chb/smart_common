        </owl:annotatedTarget>
    </owl:Axiom>
    <owl:Axiom>
        <rdfs:comment>Encounter at which vital signs were measured. This should specify a date and encounter type, at minimum.</rdfs:comment>
        <owl:annotatedSource rdf:resource="&sp;VitalSignSet"/>
        <owl:annotatedProperty rdf:resource="&rdfs;subClassOf"/>
        <owl:annotatedTarget>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&sp;encounter"/>
                <owl:allValuesFrom rdf:resource="&sp;Encounter"/>
            </owl:Restriction>
        </owl:annotatedTarget>
    </owl:Axiom>
    <owl:Axiom>
        <rdfs:comment>Patient&apos;s Temperature in Celcius. </rdfs:comment>
        <owl:annotatedSource rdf:resource="&sp;VitalSignSet"/>
        <owl:annotatedProperty rdf:resource="&rdfs;subClassOf"/>
