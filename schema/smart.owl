        <owl:annotatedProperty rdf:resource="&rdfs;subClassOf"/>
        <owl:annotatedTarget>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&sp;allergyExclusionName"/>
                <owl:allValuesFrom rdf:resource="&sp;CodedValue"/>
            </owl:Restriction>
        </owl:annotatedTarget>
    </owl:Axiom>
    


