        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&sp;provenance"/>
                <owl:allValuesFrom rdf:resource="&sp;Code"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&sp;drugName"/>
                <owl:allValuesFrom rdf:resource="&sp;CodedValue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&sp;endDate"/>
                <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&sp;quantity"/>
                <owl:allValuesFrom rdf:resource="&sp;ValueAndUnit"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
